  "main": "build/index.js",
  "scripts": {
    "storybook": "start-storybook -s ./static -p 6611",
    "build": "rimraf build && yarn copy && yarn copy-assets && yarn copy-assets-storybook && tsc && yarn cleanup",
    "watch": "run-p watch:*",
    "watch:ts": "tsc --watch",
    "watch:style": "node-sass-chokidar ./src -o ./build --watch --ext scss",
    "copy": "copyfiles -u 1 -e src/**/*.ts* -e src/**/*.md src/**/*.* build",
    "copy-assets": "copyfiles -u 4 src/local/asset-icon/counters/*.svg ../client/public/assets/counters",
    "copy-assets-storybook": "copyfiles -u 4 src/local/asset-icon/counters/*.svg static/assets/counters",
    "cleanup": "rimraf build/vendor && rimraf build/local/**/mocks && rimraf build/local/**/*.stories.* && rimraf build/local/**/*.spec.*",
    "test": "run-p test:*",
    "test:ts": "tsc --pretty",
    "test:unit": "jest --passWithNoTests",
    "lint": "run-p lint:*",
    "lint:ts": "eslint --fix src/**/*.ts*",
    "lint:styles": "stylelint --fix src/**/*.scss",
    "build-storybook": "build-storybook -s ./static -c .storybook -o ../client/build/storybook",
    "add-component": "npx buildcom -sjt -c 'scss' -m"
  },
