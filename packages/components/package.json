{
  "name": "@serge/components",
  "version": "0.3.0",
  "license": "Apache-2.0",
  "author": "foxleigh81 <alex@alexfoxleigh.com>",
  "main": "build/index.js",
  "scripts": {
    "storybook": "start-storybook -s ./static",
    "build": "rimraf build && yarn copy && tsc && yarn cleanup",
    "watch": "tsc --watch",
    "copy": "copyfiles -u 1 -e src/**/*.ts* -e src/**/*.md src/**/*.* build",
    "cleanup": "rimraf build/vendor && rimraf build/local/**/mocks && rimraf build/local/**/*.stories.* && rimraf build/local/**/*.spec.*",
    "test": "run-p test:*",
    "test:ts": "tsc --pretty",
    "test:unit": "jest --coverage",
    "lint": "run-p lint:*",
    "lint:ts": "eslint --fix src/**/*.ts*",
    "lint:styles": "stylelint src/**/*.scss",
    "build-storybook": "build-storybook -s ./static -c .storybook -o .out"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@material-ui/core": "^4.9.2",
    "@serge/custom-types": "0.3.0",
    "@serge/mocks": "0.3.0",
    "@serge/themes": "0.3.0",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@types/babel-core": "^6.25.6",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.1.2",
    "@types/node-sass": "^4.11.0",
    "@types/react-leaflet": "^2.5.1",
    "@types/react-test-renderer": "^16.9.2",
    "@types/vfile-message": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "css-modules-typescript-loader": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.1.0",
    "jest": "^25.1.0",
    "markdown-loader": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.12.0",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-dom": "^16.12.0",
    "react-leaflet": "^2.6.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.2",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "standard": "^14.3.1",
    "storybook-addon-react-docgen": "^1.2.28",
    "storybook-readme": "^5.0.8",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-css-modules": "^0.9.0",
    "stylelint-prettier": "^1.1.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  },
