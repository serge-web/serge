{
  "exclude": ["node_modules", "dist", "build", "lib", "__tests__", "**/*.test*", "**/*.scss.d.ts"],
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es5",
      "es6",
      "es7",
      "es2017",
      "ES2019"
    ],
    "types": ["vite/client", "@types/react-leaflet-markercluster", "vite-plugin-svgr/client", "jest", "@types/jest"],
    "allowJs": true,
    
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es2020",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react",
    "composite": false,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "removeComments": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "baseUrl": ".",
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./build"
  },
  "files": [
    "./typings.d.ts"
  ],
  "src/*": ["src/*"],
  "include": [
    "src"
  ]
}